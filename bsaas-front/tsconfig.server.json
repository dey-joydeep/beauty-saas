{
  "extends": "./tsconfig.app.json",
  "compilerOptions": {
    "outDir": "./dist/out-tsc/server",
    "target": "ES2022",
    "types": ["node"],
    "module": "NodeNext",
    "moduleResolution": "NodeNext",
    "importHelpers": true,
    "noEmit": false
  },
  "files": ["src/main.server.ts", "src/server.ts"],
  "angularCompilerOptions": {
    "entryModule": "./src/app/app.server.module#AppServerModule"
  }
}
