{
  "name": "bsaas-front",
  "version": "0.0.0",
  "scripts": {
    "ng": "ng",
    "start": "ng serve",
    "build": "ng build",
    "build:ssr": "ng build && ng run bsaas-front:server",
    "prerender": "cross-env NG_APP_PHASE=route-extraction ng run bsaas-front:prerender",
    "serve:prerender": "node dist/bsaas-front/server/server.mjs",
    "watch": "ng build --watch --configuration development",
    "test": "npx jest",
    "test:ci": "npx jest --ci --runInBand --coverage",
    "test:coverage": "npx jest --coverage",
    "serve:ssr": "node dist/bsaas-front/server/server.mjs",
    "lint": "npx eslint .",
    "lint:fix": "npx eslint . --fix",
    "format": "npx prettier --write .",
    "format:check": "npx prettier --check .",
    "quality:check": "node --no-warnings=ExperimentalWarning scripts/quality-check.js",
    "quality:report": "node --no-warnings=ExperimentalWarning scripts/generate-quality-report.js",
    "quality:ci": "npm run lint && npm run test:ci && npm run quality:report",
    "build:analyze": "ng build --stats-json && npx webpack-bundle-analyzer dist/bsaas-front/stats.json",
    "postinstall": "node scripts/check-node-version.js"
  },
  "engines": {
    "node": ">=18.0.0",
    "npm": ">=9.0.0"
  },
  "type": "module",
  "private": true,
  "dependencies": {
    "@angular/animations": "^19.2.7",
    "@angular/cdk": "^19.2.10",
    "@angular/common": "^19.2.0",
    "@angular/compiler": "^19.2.0",
    "@angular/core": "^19.2.0",
    "@angular/forms": "^19.2.7",
    "@angular/material": "^19.2.10",
    "@angular/platform-browser": "^19.2.0",
    "@angular/platform-browser-dynamic": "^19.2.0",
    "@angular/platform-server": "^19.2.0",
    "@angular/router": "^19.2.0",
    "@angular/ssr": "^19.2.8",
    "@ngx-translate/core": "^16.0.4",
    "@ngx-translate/http-loader": "^16.0.1",
    "chart.js": "^4.4.9",
    "express": "^4.18.2",
    "ng2-charts": "^5.0.4",
    "rxjs": "7.8.2",
    "tslib": "^2.3.0",
    "zone.js": "~0.15.0"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "^19.2.8",
    "@eslint/js": "^9.28.0",
    "@types/jasmine": "^5.1.7",
    "chalk": "^5.4.1",
    "cross-env": "^7.0.3",
    "jasmine-core": "^5.6.0",
    "jest-environment-jsdom": "^29.7.0",
    "karma": "^6.4.4",
    "karma-chrome-launcher": "^3.2.0",
    "karma-coverage": "^2.2.1",
    "karma-jasmine": "^5.1.0",
    "karma-jasmine-html-reporter": "^2.1.0",
    "prettier": "^3.5.3",
    "sass": "^1.89.2",
    "table": "^6.9.0",
    "typescript-eslint": "^8.33.1",
    "webpack-bundle-analyzer": "^4.10.2",
    "@angular/ssr": "^19.2.15"
  }
}
