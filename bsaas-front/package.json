{
  "name": "bsaas-front",
  "version": "0.0.0",
  "scripts": {
    "ng": "ng",
    "start": "ng serve",
    "build": "ng build",
    "build:ssr": "ng build && ng run bsaas-front:server",
    "prerender": "cross-env NODE_OPTIONS='--inspect' NG_APP_PHASE=route-extraction ng run bsaas-front:prerender:production",
    "prerender:debug": "cross-env NODE_OPTIONS='--inspect' NODE_OPTIONS=--max_old_space_size=8192 ng run bsaas-front:prerender:debug --verbose",
    "serve:prerender": "node dist/bsaas-front/server/server.mjs",
    "watch": "ng build --watch --configuration development",
    "test": "jest --config=jest.config.ts --passWithNoTests",
    "test:watch": "jest --config=jest.config.ts --watch",
    "test:ci": "jest --config=jest.config.ts --ci --runInBand --coverage",
    "test:coverage": "jest --config=jest.config.ts --coverage",
    "serve:ssr": "node dist/bsaas-front/server/server.mjs",
    "lint": "eslint .",
    "lint:fix": "eslint . --fix",
    "format": "prettier --write .",
    "format:check": "prettier --check .",
    "quality:check": "node --no-warnings=ExperimentalWarning scripts/quality-check.js",
    "quality:report": "node --no-warnings=ExperimentalWarning scripts/generate-quality-report.js",
    "quality:ci": "npm run lint && npm run test:ci && npm run quality:report",
    "build:analyze": "ng build --stats-json && webpack-bundle-analyzer dist/bsaas-front/stats.json",
    "postinstall": "node scripts/check-node-version.js"
  },
  "engines": {
    "node": ">=18.0.0",
    "npm": ">=9.0.0"
  },
  "type": "module",
  "private": true,
  "dependencies": {
    "@angular/animations": "^20.1.3",
    "@angular/cdk": "^20.1.3",
    "@angular/common": "^20.1.3",
    "@angular/compiler": "^20.1.3",
    "@angular/core": "^20.1.3",
    "@angular/forms": "^20.1.3",
    "@angular/material": "^20.1.3",
    "@angular/platform-browser": "^20.1.3",
    "@angular/platform-browser-dynamic": "^20.1.3",
    "@angular/platform-server": "^20.1.3",
    "@angular/router": "^20.1.3",
    "@ngx-translate/core": "^16.0.4",
    "@ngx-translate/http-loader": "^16.0.1",
    "chart.js": "^4.5.0",
    "express": "4.21.2",
    "ng2-charts": "^8.0.0",
    "rxjs": "^7.4.0",
    "tslib": "^2.8.1",
    "zone.js": "~0.15.1"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "20.1.4",
    "@angular/cli": "^20.1.4",
    "@angular/compiler-cli": "^20.1.4",
    "@angular/ssr": "^20.1.4",
    "@eslint/js": "9.32.0",
    "@types/jest": "29.5.2",
    "@types/node": "24.1.0",
    "angular-eslint": "^20.1.1",
    "chalk": "5.4.1",
    "cross-env": "10.0.0",
    "jest": "29.6.4",
    "jest-preset-angular": "14.6.0",
    "prettier": "3.6.2",
    "sass": "1.89.2",
    "table": "6.9.0",
    "ts-jest": "29.4.0",
    "ts-node": "10.9.2",
    "typescript": "5.8.3",
    "typescript-eslint": "^8.38.0",
    "webpack-bundle-analyzer": "4.10.2"
  }
}
