{
  "compileOnSave": false,
  "compilerOptions": {
    "noFallthroughCasesInSwitch": true,
    "noEmitOnError": true,
    "forceConsistentCasingInFileNames": true,
    "target": "ES2022",
    "baseUrl": ".",
    "isolatedModules": true,
    "useDefineForClassFields": true,
    "importHelpers": true,
    "emitDecoratorMetadata": true,
    "module": "ES2022",
    "lib": [
      "ES2022",
      "dom"
    ],
    "esModuleInterop": true,
    "allowJs": false,
    "moduleDetection": "force",
    "strict": true,
    "moduleResolution": "node",
    "paths": {
      "@app/*": [
        "apps/api/src/*"
      ],
      "@beauty-saas/core": [
        "libs/core/src/index.ts"
      ],
      "@beauty-saas/core/*": [
        "libs/core/src/*"
      ],
      "@beauty-saas/data-access": [
        "libs/data-access/src/index.ts"
      ],
      "@beauty-saas/data-access/*": [
        "libs/data-access/src/*"
      ],
      "@beauty-saas/shared": [
        "libs/shared/src/index.ts"
      ],
      "@beauty-saas/shared/*": [
        "libs/shared/src/*"
      ],
      "@beauty-saas/web-ui": [
        "libs/web/ui/dist"
      ],
      "@beauty-saas/web-ui/*": [
        "libs/web/ui/dist/*"
      ],
      "@beauty-saas/web-admin/auth": [
        "libs/web/admin/auth/dist"
      ],
      "@beauty-saas/web-admin/auth/*": [
        "libs/web/admin/auth/dist/*"
      ],
      "@beauty-saas/web-config": [
        "libs/web/config/dist"
      ],
      "@beauty-saas/web-config/*": [
        "libs/web/config/dist/*"
      ],
      "@beauty-saas/web-core/auth": [
        "libs/web/core/auth/dist"
      ],
      "@beauty-saas/web-core/auth/*": [
        "libs/web/core/auth/dist/*"
      ],
      "@beauty-saas/web-core/http": [
        "libs/web/core/http/dist"
      ],
      "@beauty-saas/web-core/http/*": [
        "libs/web/core/http/dist/*"
      ],
      "@beauty-saas/web-core/testing": [
        "libs/web/core/testing/dist"
      ],
      "@beauty-saas/web-core/testing/*": [
        "libs/web/core/testing/dist/*"
      ],
      "@beauty-saas/web-customer/auth": [
        "libs/web/customer/auth/dist"
      ],
      "@beauty-saas/web-customer/auth/*": [
        "libs/web/customer/auth/dist/*"
      ],
      "@beauty-saas/web-partner/auth": [
        "libs/web/partner/auth/dist"
      ],
      "@beauty-saas/web-partner/auth/*": [
        "libs/web/partner/auth/dist/*"
      ]
    },
    "types": [
      "node",
      "jest"
    ],
    "experimentalDecorators": true,
    "sourceMap": true,
    "noUnusedParameters": true,
    "noUnusedLocals": true,
    "noImplicitReturns": true,
    "declarationMap": true,
    "declaration": true,
    "noImplicitAny": true,
    "resolveJsonModule": true
  },
  "exclude": [
    "node_modules",
    "dist"
  ],
  "angularCompilerOptions": {
    "enableI18nLegacyMessageIdFormat": false,
    "strictInjectionParameters": true,
    "strictInputAccessModifiers": true,
    "strictTemplates": true
  }
}