{
  "name": "beauty-saas",
  "version": "1.0.0",
  "private": true,
  "type": "module",
  "workspaces": [
    "apps/*",
    "libs/*"
  ],
  "scripts": {
    "postinstall": "node scripts/check-versions/node-version.js",
    "dev:frontend": "nx serve bsaas-front",
    "dev:api": "nx serve bsaas-back",
    "dev:docs": "nx serve bsaas-docs",
    "build": "nx run-many --target=build --all",
    "build:prod": "nx run-many --target=build --all --configuration=production",
    "build:affected": "nx affected:build",
    "build:frontend": "nx build bsaas-front",
    "build:api": "nx build bsaas-back",
    "build:docs": "nx build bsaas-docs",
    "test": "nx run-many --target=test --all",
    "test:watch": "nx run-many --target=test --all --watch",
    "test:affected": "nx affected:test",
    "test:coverage": "nx run-many --target=test --all --code-coverage",
    "test:frontend": "nx test bsaas-front",
    "test:api": "nx test bsaas-back",
    "lint": "nx run-many --target=lint --all",
    "lint:fix": "nx run-many --target=lint --all --fix",
    "lint:affected": "nx affected:lint",
    "lint:frontend": "nx lint bsaas-front",
    "lint:api": "nx lint bsaas-back",
    "format": "prettier --write .",
    "format:check": "prettier --check .",
    "deps:check": "npx npm-check-updates -u",
    "deps:update": "npm run deps:check && npm install",
    "deps:audit": "npm audit",
    "deps:audit:fix": "npm audit fix",
    "nx:graph": "nx graph",
    "nx:reset": "nx reset",
    "nx:generate": "nx generate",
    "nx:migrate": "nx migrate",
    "nx:affected:apps": "nx affected:apps",
    "nx:affected:libs": "nx affected:libs",
    "nx:affected:build": "nx affected:build",
    "nx:affected:test": "nx affected:test",
    "nx:affected:lint": "nx affected:lint",
    "docs:generate": "node ./scripts/generate-readmes.js",
    "docs:serve": "nx serve bsaas-docs",
    "docs:build": "nx build bsaas-docs"
  },
  "devDependencies": {
    "@angular-devkit/architect": "^0.2001.4",
    "@angular-devkit/build-angular": "^20.1.4",
    "@angular/ssr": "^20.1.4",
    "@jimp/types": "^1.6.0",
    "@nx/angular": "^21.3.11",
    "@nx/eslint": "21.3.10",
    "@nx/jest": "21.3.10",
    "@nx/js": "21.3.10",
    "@nx/nest": "^21.3.11",
    "@nx/node": "21.3.10",
    "@nx/webpack": "^21.3.11",
    "@nx/workspace": "^21.3.11",
    "@types/express": "^5.0.3",
    "@types/jest": "^29.5.2",
    "@types/node": "^24.1.0",
    "@types/passport": "^1.0.17",
    "@typescript-eslint/eslint-plugin": "^8.38.0",
    "@typescript-eslint/parser": "^8.38.0",
    "eslint": "^9.32.0",
    "eslint-config-google": "^0.14.0",
    "eslint-config-prettier": "^8.8.0",
    "eslint-plugin-import": "^2.32.0",
    "eslint-plugin-jest": "^29.0.1",
    "husky": "^9.1.7",
    "jest": "^29.6.4",
    "lint-staged": "^16.1.2",
    "nx": "^21.3.11",
    "prettier": "^3.6.2",
    "typescript": "^5.8.3"
  },
  "dependencies": {
    "@angular/animations": "^20.1.3",
    "@angular/cdk": "^20.1.3",
    "@angular/common": "^20.1.3",
    "@angular/compiler": "^20.1.3",
    "@angular/core": "^20.1.3",
    "@angular/forms": "^20.1.3",
    "@angular/material": "^20.1.3",
    "@angular/platform-browser": "^20.1.3",
    "@angular/platform-browser-dynamic": "^20.1.3",
    "@angular/platform-server": "^20.1.3",
    "@angular/router": "^20.1.3",
    "@nestjs/common": "^10.0.0",
    "@nestjs/core": "^10.0.0",
    "@nestjs/platform-express": "^10.0.0",
    "@ngx-translate/core": "^16.0.4",
    "@ngx-translate/http-loader": "^16.0.1",
    "@nx/angular": "^21.3.11",
    "@prisma/client": "^6.12.0",
    "bcryptjs": "^3.0.2",
    "chart.js": "^4.5.0",
    "express": "^4.21.2",
    "ng2-charts": "^8.0.0",
    "reflect-metadata": "^0.1.13",
    "rxjs": "^7.8.1",
    "tslib": "^2.6.2",
    "zone.js": "~0.15.1"
  },
  "engines": {
    "node": ">=18.0.0",
    "npm": ">=9.0.0"
  },
  "nx": {}
}
