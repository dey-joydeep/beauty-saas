{
  "name": "beauty-saas",
  "version": "1.0.0",
  "private": true,
  "type": "module",
  "workspaces": [
    "apps/*",
    "libs/*"
  ],
  "scripts": {
    "postinstall": "node scripts/check-versions/node-version.js",
    "dev:frontend": "nx serve bsaas-front",
    "dev:api": "nx serve bsaas-back",
    "dev:docs": "nx serve bsaas-docs",
    "build": "nx run-many --target=build --all",
    "build:prod": "nx run-many --target=build --all --configuration=production",
    "build:affected": "nx affected:build",
    "build:frontend": "nx build bsaas-front",
    "build:api": "nx build bsaas-back",
    "build:docs": "nx build bsaas-docs",
    "test": "nx run-many --target=test --all",
    "test:watch": "nx run-many --target=test --all --watch",
    "test:affected": "nx affected:test",
    "test:coverage": "nx run-many --target=test --all --code-coverage",
    "test:frontend": "nx test bsaas-front",
    "test:api": "nx test bsaas-back",
    "lint": "nx run-many --target=lint --all",
    "lint:fix": "nx run-many --target=lint --all --fix",
    "lint:affected": "nx affected:lint",
    "lint:frontend": "nx lint bsaas-front",
    "lint:api": "nx lint bsaas-back",
    "format": "prettier --write .",
    "format:check": "prettier --check .",
    "deps:check": "npx npm-check-updates -u",
    "deps:update": "npm run deps:check && npm install",
    "deps:audit": "npm audit",
    "deps:audit:fix": "npm audit fix",
    "nx:graph": "nx graph",
    "nx:reset": "nx reset",
    "nx:generate": "nx generate",
    "nx:migrate": "nx migrate",
    "nx:affected:apps": "nx affected:apps",
    "nx:affected:libs": "nx affected:libs",
    "nx:affected:build": "nx affected:build",
    "nx:affected:test": "nx affected:test",
    "nx:affected:lint": "nx affected:lint",
    "docs:generate": "node ./scripts/generate-readmes.js",
    "docs:serve": "nx serve bsaas-docs",
    "docs:build": "nx build bsaas-docs"
  },
