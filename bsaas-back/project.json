{
  "name": "bsaas-back",
  "$schema": "../node_modules/nx/schemas/project-schema.json",
  "projectType": "application",
  "sourceRoot": "bsaas-back/src",
  "targets": {
    "build": {
      "executor": "nx:run-commands",
      "outputs": ["{workspaceRoot}/dist/bsaas-back"],
      "options": {
        "command": "npm run build --prefix bsaas-back",
        "cwd": "."
      },
      "configurations": {
        "production": {}
      }
    },
    "serve": {
      "executor": "@nx/js:node",
      "options": {
        "buildTarget": "bsaas-back:build"
      },
      "configurations": {
        "production": {
          "buildTarget": "bsaas-back:build:production"
        }
      }
    },
    "test": {
      "executor": "@nx/jest:jest",
      "outputs": ["{workspaceRoot}/coverage/{projectRoot}"],
      "options": {
        "jestConfig": "bsaas-back/jest.config.ts",
        "passWithNoTests": true
      }
    },
    "lint": {
      "executor": "@nx/eslint:lint",
      "outputs": ["{options.outputFile}"],
      "options": {
        "lintFilePatterns": [
          "bsaas-back/**/*.ts",
          "bsaas-back/**/*.tsx"
        ]
      }
    }
  },
  "tags": ["type:app", "scope:backend"]
}
