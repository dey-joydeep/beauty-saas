{
    "extends": "./tsconfig.json",
    "compilerOptions": {
        "outDir": "../../../dist/out-tsc",
        "module": "commonjs",
        "moduleResolution": "node",
        "types": ["jest", "node"],
        "esModuleInterop": true,
        "target": "es2022",
        "isolatedModules": false,
        "useDefineForClassFields": false
    },
    "include": ["**/*.spec.ts", "**/*.test.ts", "**/*.d.ts", "**/*.fixture.ts"]
}
