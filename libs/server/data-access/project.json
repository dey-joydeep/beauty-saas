{
    "name": "server-data-access",
    "$schema": "../../../node_modules/nx/schemas/project-schema.json",
    "sourceRoot": "libs/server/data-access/src",
    "projectType": "library",
    "tags": ["type:lib", "platform:server", "scope:server-data", "scope:server", "type:data"],
    "targets": {
        "build": {
            "executor": "@nx/js:tsc",
            "outputs": ["{options.outputPath}"],
            "options": {
                "outputPath": "dist/libs/server/data-access",
                "main": "libs/server/data-access/src/index.ts",
                "tsConfig": "libs/server/data-access/tsconfig.lib.json",
                "assets": ["libs/server/data-access/src/schema.prisma", "libs/server/data-access/src/migrations/**/*"],
                "updateBuildableProjectDepsInPackageJson": true,
                "buildableProjectDepsInPackageJsonType": "dependencies",
                "rootDir": "libs/server/data-access/src"
            },
            "configurations": {
                "production": {}
            }
        },
        "lint": {
            "executor": "@nx/eslint:lint",
            "outputs": ["{options.outputFile}"],
            "options": {
                "lintFilePatterns": ["libs/server/data-access/**/*.ts"]
            }
        },
        "type-check": {
            "executor": "nx:run-commands",
            "options": {
                "command": "tsc -p libs/server/data-access/tsconfig.lib.json --noEmit"
            }
        },
        "test": {
            "executor": "@nx/jest:jest",
            "outputs": ["{workspaceRoot}/coverage/{projectRoot}"],
            "options": {
                "jestConfig": "libs/server/data-access/jest.config.ts",
                "passWithNoTests": true
            }
        }
    }
}
