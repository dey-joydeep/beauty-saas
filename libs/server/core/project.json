{
  "name": "server-core",
  "sourceRoot": "libs/server/core/src",
  "tags": [
    "scope:server",
    "type:core"
  ],
  "targets": {
    "lint": {
      "outputs": [
        "{options.outputFile}"
      ],
      "executor": "@nx/eslint:lint",
      "options": {
        "lintFilePatterns": [
          "libs/server/core/**/*.ts"
        ]
      }
    },
    "build": {
      "outputs": [
        "{options.outputPath}"
      ],
      "executor": "@nx/js:tsc",
      "options": {
        "main": "libs/server/core/src/index.ts",
        "tsConfig": "libs/server/core/tsconfig.lib.json",
        "packageJson": "libs/server/core/package.json",
        "assets": [],
        "outputPath": "dist/libs/server/core"
      }
    },
    "type-check": {
      "executor": "nx:run-commands",
      "options": {
        "command": "tsc -p libs/server/core/tsconfig.lib.json --noEmit"
      }
    },
    "test": {
      "outputs": [
        "{workspaceRoot}/coverage/{projectRoot}"
      ],
      "executor": "@nx/jest:jest",
      "options": {
        "passWithNoTests": true,
        "jestConfig": "libs/server/core/jest.config.ts",
        "codeCoverage": true,
        "coverageReporters": [
          "text",
          "lcov"
        ]
      }
    }
  },
  "projectType": "library",
  "prefix": "beauty-saas"
}
