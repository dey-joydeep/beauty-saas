{
    "name": "@beauty-saas/features",
    "projectType": "library",
    "sourceRoot": "libs/features/src",
    "prefix": "beauty-saas",
    "tags": [
        "scope:features",
        "type:features"
    ],
    "targets": {
        "build": {
            "executor": "@nx/js:tsc",
            "outputs": [
                "{options.outputPath}"
            ],
            "options": {
                "outputPath": "dist/libs/features",
                "tsConfig": "libs/features/tsconfig.lib.json",
                "packageJson": "libs/features/package.json",
                "main": "libs/features/src/index.ts",
                "assets": [
                    "libs/features/*.md"
                ]
            }
        },
        "lint": {
            "executor": "@nx/linter:eslint",
            "outputs": [
                "{options.outputFile}"
            ],
            "options": {
                "lintFilePatterns": [
                    "libs/features/**/*.ts",
                    "libs/features/**/*.html"
                ]
            }
        },
        "type-check": {
      "executor": "nx:run-commands",
      "options": {
        "command": "tsc -p libs/features/tsconfig.lib.json --noEmit"
      }
    },
    "test": {
            "executor": "@nx/jest:jest",
            "outputs": [
                "{workspaceRoot}/coverage/libs/features"
            ],
            "options": {
                "jestConfig": "jest.preset.ts",
                "passWithNoTests": true
            }
        }
    }
}