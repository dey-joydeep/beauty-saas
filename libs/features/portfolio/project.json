{
  "name": "portfolio",
  "$schema": "../../../node_modules/nx/schemas/project-schema.json",
  "projectType": "library",
  "sourceRoot": "libs/features/portfolio/src",
  "prefix": "app",
  "targets": {
    "build": {
      "executor": "@nx/js:tsc",
      "outputs": ["{options.outputPath}"],
      "options": {
        "outputPath": "dist/libs/features/portfolio",
        "tsConfig": "libs/features/portfolio/tsconfig.lib.json",
        "packageJson": "libs/features/portfolio/package.json",
        "main": "libs/features/portfolio/src/index.ts",
        "assets": [],
        "buildableProjectDepsInPackageJsonType": "dependencies"
      },
      "configurations": {
        "production": {
          "tsConfig": "libs/features/portfolio/tsconfig.lib.prod.json"
        },
        "development": {
          "tsConfig": "libs/features/portfolio/tsconfig.lib.json"
        }
      },
      "defaultConfiguration": "production"
    },
    "type-check": {
      "executor": "nx:run-commands",
      "options": {
        "command": "tsc -p libs/features/portfolio/tsconfig.lib.json --noEmit"
      }
    },
    "lint": {
      "executor": "@nx/eslint:lint",
      "outputs": ["{options.outputFile}"],
      "options": {
        "lintFilePatterns": ["libs/features/portfolio/**/*.ts"]
      }
    },
    "test": {
      "executor": "@nx/jest:jest",
      "outputs": ["{workspaceRoot}/coverage/{projectRoot}"],
      "options": {
        "jestConfig": "libs/features/portfolio/jest.config.ts",
        "passWithNoTests": true,
        "codeCoverage": true,
        "coverageReporters": ["text", "lcov"],
        "coverageDirectory": "coverage/libs/features/portfolio"
      }
    },
    "deploy": {
      "executor": "@nx/workspace:run-commands",
      "options": {
        "command": "npm publish {projectRoot}/dist --access public",
        "parallel": true
      },
      "dependsOn": ["build"]
    }
  },
  "tags": ["type:lib", "platform:server", "scope:server-feature"],
  "implicitDependencies": ["core"],
  "dependencies": [
    {
      "target": "data-access",
      "projects": "data-access"
    }
  ]
}
