{
  "extends": "./tsconfig.json",
  "compilerOptions": {
    "outDir": "../../../../dist/out-tsc/libs/web/partner/auth",
    "declaration": true,
    "declarationMap": true,
    "inlineSources": true,
    "types": [],
    "rootDir": "./src",
    "paths": {
      "@beauty-saas/web-core/http": ["libs/web/core/http/dist/index.d.ts"],
      "@beauty-saas/web-core/http/*": ["libs/web/core/http/dist/*"],
      "@beauty-saas/web-core-http": ["libs/web/core/http/dist/index.d.ts"],
      "@beauty-saas/web-core-http/*": ["libs/web/core/http/dist/*"],
      "@beauty-saas/web-core/auth": ["libs/web/core/auth/dist/index.d.ts"],
      "@beauty-saas/web-core/auth/*": ["libs/web/core/auth/dist/*"],
      "@beauty-saas/web-core-auth": ["libs/web/core/auth/dist/index.d.ts"],
      "@beauty-saas/web-core-auth/*": ["libs/web/core/auth/dist/*"],
      "@beauty-saas/web-config": ["libs/web/config/dist/index.d.ts"],
      "@beauty-saas/web-config/*": ["libs/web/config/dist/*"]
    }
  },
  "angularCompilerOptions": { "compilationMode": "partial" },
  "exclude": [
    "src/test-setup.ts",
    "**/*.spec.ts",
    "jest.config.ts",
    "**/*.test.ts"
  ],
  "include": ["src/**/*.ts"]
}
