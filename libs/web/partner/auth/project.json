{
    "name": "web-partner-auth",
    "$schema": "../../../../node_modules/nx/schemas/project-schema.json",
    "sourceRoot": "libs/web/partner/auth/src",
    "prefix": "lib",
    "projectType": "library",
    "targets": {
        "test": {
            "executor": "@nx/jest:jest",
            "outputs": ["{workspaceRoot}/coverage/{projectRoot}"],
            "options": {
                "jestConfig": "libs/web/partner/auth/jest.config.ts"
            }
        },
        "lint": {
            "executor": "@nx/eslint:lint"
        },
        "build": {
            "outputs": ["{workspaceRoot}/dist/libs/web/partner/auth"],
            "executor": "@nx/angular:package",
            "options": {
                "tsConfig": "libs/web/partner/auth/tsconfig.lib.json",
                "project": "libs/web/partner/auth/ng-package.json"
            },
            "dependsOn": ["^build"]
        }
    },
    "tags": ["scope:web-app", "type:feature"]
}
