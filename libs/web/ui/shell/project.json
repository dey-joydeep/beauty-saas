{
  "name": "shell",
  "$schema": "../../../../node_modules/nx/schemas/project-schema.json",
  "sourceRoot": "libs/web/ui/shell/src",
  "prefix": "lib",
  "projectType": "library",
  "tags": ["scope:web", "type:ui"],
  "targets": {
    "build": {
      "executor": "@nx/angular:package",
      "outputs": [
        "{projectRoot}/dist"
      ],
      "options": {
        "tsConfig": "libs/web/ui/shell/tsconfig.lib.json",
        "project": "libs/web/ui/shell/ng-package.json"
      },
      "dependsOn": [
        "^build"
      ],
      "configurations": {
        "production": {
          "tsConfig": "libs/web/ui/shell/tsconfig.lib.prod.json"
        },
        "development": {
          "tsConfig": "libs/web/ui/shell/tsconfig.lib.json"
        }
      },
      "defaultConfiguration": "production"
    },
    "test": {
      "executor": "@nx/jest:jest",
      "outputs": [
        "{workspaceRoot}/coverage/{projectRoot}"
      ],
      "options": {
        "jestConfig": "libs/web/ui/shell/jest.config.ts",
        "passWithNoTests": true
      },
      "configurations": {
        "ci": {
          "ci": true,
          "codeCoverage": true
        }
      }
    },
    "type-check": {
      "executor": "nx:run-commands",
      "options": {
        "command": "tsc -p libs/web/ui/shell/tsconfig.lib.json --noEmit"
      }
    },
    "lint": {
      "executor": "@nx/eslint:lint"
    }
  }
}
