{
    "name": "http",
    "$schema": "../../../../node_modules/nx/schemas/project-schema.json",
    "sourceRoot": "libs/web/core/http/src",
    "prefix": "lib",
    "projectType": "library",
    "tags": [
        "type:lib",
        "platform:web",
        "scope:web-core"
    ],
    "targets": {
        "test": {
            "executor": "@nx/jest:jest",
            "outputs": [
                "{workspaceRoot}/coverage/{projectRoot}"
            ],
            "options": {
                "jestConfig": "libs/web/core/http/jest.config.ts",
                "passWithNoTests": true
            },
            "configurations": {
                "ci": {
                    "ci": true,
                    "codeCoverage": true
                }
            }
        },
        "lint": {
            "executor": "@nx/eslint:lint"
        },
        "type-check": {
            "executor": "nx:run-commands",
            "options": {
                "command": "tsc -p libs/web/core/http/tsconfig.lib.json --noEmit"
            }
        },
        "build": {
            "outputs": [
                "{workspaceRoot}/dist/{projectRoot}"
            ],
            "executor": "@nx/angular:package",
            "options": {
                "tsConfig": "libs/web/core/http/tsconfig.lib.json",
                "project": "libs/web/core/http/ng-package.json"
            }
        }
    }
}