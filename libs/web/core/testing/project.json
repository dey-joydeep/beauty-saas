{
    "name": "testing",
    "$schema": "../../../../node_modules/nx/schemas/project-schema.json",
    "sourceRoot": "libs/web/core/testing/src",
    "prefix": "lib",
    "projectType": "library",
    "tags": [
        "type:lib",
        "platform:web",
        "scope:web-core"
    ],
    "targets": {
        "test": {
            "executor": "@nx/jest:jest",
            "outputs": [
                "{workspaceRoot}/coverage/{projectRoot}"
            ],
            "options": {
                "jestConfig": "libs/web/core/testing/jest.config.ts"
            }
        },
        "lint": {
            "executor": "@nx/eslint:lint"
        },
        "build": {
            "outputs": [
                "{projectRoot}/dist"
            ],
            "executor": "@nx/angular:package",
            "options": {
                "tsConfig": "libs/web/core/testing/tsconfig.lib.json",
                "project": "libs/web/core/testing/ng-package.json"
            },
            "dependsOn": [
                "^build"
            ]
        }
    }
}