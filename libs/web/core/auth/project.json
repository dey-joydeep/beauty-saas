{
    "name": "auth",
    "$schema": "../../../../node_modules/nx/schemas/project-schema.json",
    "sourceRoot": "libs/web/core/auth/src",
    "prefix": "lib",
    "projectType": "library",
    "tags": [
        "type: lib",
        "platform:web",
        "scope:web-core"
    ],
    "targets": {
        "test": {
            "executor": "@nx/jest:jest",
            "outputs": [
                "{workspaceRoot}/coverage/{projectRoot}"
            ],
            "options": {
                "jestConfig": "libs/web/core/auth/jest.config.ts",
                "passWithNoTests": true
            },
            "configurations": {
                "ci": {
                    "ci": true,
                    "codeCoverage": true
                }
            }
        },
        "lint": {
            "executor": "@nx/eslint:lint"
        },
        "type-check": {
            "executor": "nx:run-commands",
            "options": {
                "command": "tsc -p libs/web/core/auth/tsconfig.lib.json --noEmit"
            }
        },
        "build": {
            "outputs": [
                "{projectRoot}/dist"
            ],
            "executor": "@nx/angular:package",
            "options": {
                "tsConfig": "libs/web/core/auth/tsconfig.lib.json",
                "project": "libs/web/core/auth/ng-package.json"
            },
            "dependsOn": [
                "^build"
            ]
        }
    }
}