{
    "name": "customer-auth",
    "$schema": "../../../../node_modules/nx/schemas/project-schema.json",
    "sourceRoot": "libs/web/customer/auth/src",
    "prefix": "lib",
    "projectType": "library",
    "targets": {
        "test": {
            "executor": "@nx/jest:jest",
            "outputs": [
                "{workspaceRoot}/coverage/{projectRoot}"
            ],
            "options": {
                "jestConfig": "libs/web/customer/auth/jest.config.ts"
            }
        },
        "lint": {
            "executor": "@nx/eslint:lint"
        },
        "build": {
            "outputs": [
                "{projectRoot}/dist"
            ],
            "executor": "@nx/angular:package",
            "options": {
                "tsConfig": "libs/web/customer/auth/tsconfig.lib.json",
                "project": "libs/web/customer/auth/ng-package.json"
            },
            "dependsOn": [
                "^build"
            ]
        }
    },
    "tags": [
        "type:lib",
        "platform:web",
        "scope:web-feature-auth"
    ]
}