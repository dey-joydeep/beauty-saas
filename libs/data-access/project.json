{
  "name": "data-access",
  "$schema": "../../node_modules/nx/schemas/project-schema.json",
  "sourceRoot": "libs/data-access/src",
  "projectType": "library",
  "tags": [
    "scope:data-access",
    "type:data-access",
    "layer:data"
  ],
  "targets": {
    "build": {
      "executor": "@nx/js:tsc",
      "outputs": ["{options.outputPath}"],
      "options": {
        "outputPath": "dist/libs/data-access",
        "main": "libs/data-access/src/index.ts",
        "tsConfig": "libs/data-access/tsconfig.lib.json",
        "assets": [
          "libs/data-access/prisma/schema.prisma",
          "libs/data-access/prisma/migrations/**/*"
        ]
      }
    },
    "lint": {
      "executor": "@nx/linter:eslint",
      "outputs": [
        "{options.outputFile}"
      ],
      "options": {
        "lintFilePatterns": [
          "libs/data-access/**/*.ts"
        ]
      }
    },
    "type-check": {
      "executor": "nx:run-commands",
      "options": {
        "command": "tsc -p libs/data-access/tsconfig.lib.json --noEmit"
      }
    },
    "test": {
      "executor": "@nx/jest:jest",
      "outputs": [
        "{workspaceRoot}/coverage/{projectRoot}"
      ],
      "options": {
        "jestConfig": "jest.preset.ts",
        "passWithNoTests": true
      }
    }
  }
}